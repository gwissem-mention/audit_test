{% extends 'NodevoAdminBundle::admin.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets output="compiled/MoxieManager_Admin.css" filter="cssrewrite"
        'bundles/nodevogestionnairemedia/css/MoxieManager_Admin.css'
    %}
        <link rel="stylesheet" type="text/css" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block h1Title %}
    {{ 'nodevo_gestionnaire_media.titre'|trans }}
{% endblock %}
{% block title %}{{ 'nodevo_gestionnaire_media.titre'|trans }}{% endblock %}

{% block body %}
    <div class="col-sm-12">
        <div class="panel panel-midnightblue panel-moxiemanager">
            <div class="panel-body" >

                <div id="moxiemanager_conteneur">
                    <iframe src="{{ url('nodevo_gestionnaire_media_moxiemanager_explorateur') }}" width="100%" height="600"></iframe>
                </div>

            </div>
        </div>
    </div>
{% endblock %}

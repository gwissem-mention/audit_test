{% extends 'NodevoAdminBundle::admin.html.twig' %}

{% block h1Title %}
    Fiche RefusCandidature
{% endblock %}

{% block toolbarButton %}
    <a class="btn btn-default" href="{{ path('hopitalnumerique_user_refuscandidature') }}" title="Retour Ã  la liste"><i class="fa fa-reply"></i></a>
    {% if refuscandidature.id %}
        <a href="{{ path('hopitalnumerique_user_refuscandidature_edit', { 'id': refuscandidature.id }) }}" class="btn btn-default" title="Editer"><i class="fa fa-edit"></i></a>
    {% endif %}
{% endblock %}

{% block body %}
    <div class="col-sm-12">
        <div class="panel panel-midnightblue">
            <div class="panel-heading">
                <h4>Informations</h4>
            </div>

            <div class="panel-body" >
                <p>
                    <strong>Id : </strong>
                    {{ refuscandidature.id }}
                </p>
                <p>
                    <strong>Motifrefus : </strong>
                    {{ refuscandidature.motifRefus }}
                </p>
                <p>
                    <strong>Daterefus : </strong>
                    {{ refuscandidature.dateRefus }}
                </p>
            </div>
        </div>
    </div>
{% endblock %}

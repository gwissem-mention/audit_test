{% extends 'HopitalNumeriqueInterventionBundle:Admin:layout.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets output="compiled/hopitalnumerique-intervention-demande-admin-form-edit.css" filter="cssrewrite, ?yui_css"
        "@select2_css" 
        "@validationEngine_css"
    %}
        <link rel="stylesheet" href="{{ asset_url }}">
    {% endstylesheets %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {% javascripts output="compiled/hopitalnumerique-intervention-demande-admin-form-edit.js" 
        "@select2_js" 
        "@validationEngine_js"
        'bundles/hopitalnumeriqueintervention/js/script.js'
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
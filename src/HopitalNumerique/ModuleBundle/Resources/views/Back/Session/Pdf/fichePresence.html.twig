<meta charset="utf-8">
<table width="740px" style="margin:0 auto; border:1px solid #e3e3e3;" >
    {# Titre #}
    <tr align="center" style="font-size:14px;border:1px solid #e3e3e3;">
        <td>
            <strong>FEUILLE D'EMERGEMENT - {{ session.dateSession|date('d/m/Y') }}</strong>
        </td>
    </tr>
    {# Ligne du module #}
    <tr style="border:1px solid #e3e3e3;">
        <td>
            <strong>{{ session.module.titre }}</strong>
        </td>
    </tr>
</table>
{# Ligne de séparation #}
<div style="background: #5d5d5d;height:20px; width:740px"></div>
<table width="740px" style="margin:0 auto; border-collapse:collapse;">
    <thead>
        <tr class="grid-row-titles">
            <th style="font-size:12px;vertical-align:middle;border:1px solid #e3e3e3;margin: 0; padding: 0;" height="25px"><strong>REGION</strong></th>
            <th style="font-size:12px;vertical-align:middle;border:1px solid #e3e3e3;margin: 0; padding: 0;" height="25px"><strong>ETABLISSEMENT</strong></th>
            <th style="width:72px;font-size:12px;vertical-align:middle;border:1px solid #e3e3e3;margin: 0; padding: 0;" height="25px"><strong>PRENOM</strong></th>
            <th style="width:70px;font-size:12px;vertical-align:middle;border:1px solid #e3e3e3;margin: 0; padding: 0;" height="25px"><strong>NOM</strong></th>
            <th style="width:120px;font-size:12px;vertical-align:middle;border:1px solid #e3e3e3;margin: 0; padding: 0;" height="25px"><strong>SIGNATURE</strong></th>
            <th style="width:120px;font-size:12px;vertical-align:middle;border:1px solid #e3e3e3;"><strong>OBSERVATIONS</strong></th>
        </tr>
    </thead>
    <tbody>
        {% for inscription in inscriptions %}
            <tr>
                <td style="border:1px solid #e3e3e3;font-size:11px;" height="30px">
                    {% if inscription.user.region is not null and inscription.user.region is not empty %}
                        {{ inscription.user.region.libelle }}
                    {% else %}
                        - Région renseignée -
                    {% endif %}
                </td>
                <td style="border:1px solid #e3e3e3;font-size:11px;">
                    {% if inscription.user.etablissementRattachementSante is not null %}
                        {{ inscription.user.etablissementRattachementSante.nom }}
                    {% else %}
                        {{ inscription.user.autreStructureRattachementSante }}
                    {% endif %}
                </td>
                <td style="border:1px solid #e3e3e3;font-size:11px;">
                    {{ inscription.user.prenom }}
                </td>
                <td style="border:1px solid #e3e3e3;font-size:11px;">
                    {{ inscription.user.nom }}
                </td>
                <td style="border:1px solid #e3e3e3;font-size:11px;">
                    
                </td>
                <td style="border:1px solid #e3e3e3;font-size:11px;">
                    
                </td>
            </tr>
        {% endfor %}  
    </tbody>
</table>
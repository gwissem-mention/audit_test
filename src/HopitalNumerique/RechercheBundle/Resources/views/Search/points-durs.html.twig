<div class="row" {% if count >= showMorePointsDurs %}style="display:none"{% endif %} >
    <div class="col-md-10">
        <h4>
            {% if objet['updated'] %}
                <i class="fa fa-refresh"></i>
            {% elseif objet['new'] %}
                <i class="fa fa-bullhorn rose"></i>
            {% endif %}

            {% if objet['objet'] is not null %}
                <a href="{{path('hopital_numerique_publication_publication_contenu', {'id':objet['objet'],'alias':objet['aliasO'],'idc':objet['id'],'aliasc':objet['aliasC']})}}" >
            {% else %}
                <a href="{{path('hopital_numerique_publication_publication_objet', {'id':objet['id'],'alias':objet['alias']})}}" >
            {% endif %}
                {{objet['titre']}}
            </a>
        </h4>
    </div>
    
    <div class="col-md-2">
        {% if objet['synthese'] is not null %}
            <a class="synthese fancybox.iframe" href="{{path('hopital_numerique_publication_synthese',{'id':objet['synthese']})}}">synth√®se</a>
        {% endif %}
    </div>
    <div class="col-md-12">
        <p>{{ objet['resume'] }}</p>
    </div>
</div>
{% macro linkGroup(value, groupId) %}
    <a
        href="{{ path('hopitalnumerique_communautepratique_groupe_view', [
            'groupe' => groupId
        ]) }}"
        target="_blank"
    >{{ value }}</a>
{% endmacro %}

{% macro linkForm(value, formId) %}
    <a
        href="{{ path('hopitalnumerique_communautepratique_fiche_view', [
            'fiche' => formId
        ]) }}"
        target="_blank"
    >{{ value }}</a>
{% endmacro %}
